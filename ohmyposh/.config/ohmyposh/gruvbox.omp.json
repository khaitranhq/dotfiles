{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UserName }} ",
          "foreground": "#FBF1C7",
          "background": "#B16286",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uea83  {{ .Path }} ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b0",
          "background": "#CC241D",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#1b1b1b",
          "powerline_symbol": "\ue0b0",
          "background": "#FABD2F",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FE8019{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FB4934{{ end }}",
            "{{ if gt .Ahead 0 }}#D3869B{{ end }}",
            "{{ if gt .Behind 0 }}#D3869B{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b0",
          "background": "#98971A",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b0",
          "background": "#458588",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b0",
          "background": "#83A598",
          "type": "julia",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#1b1b1b",
          "powerline_symbol": "\ue0b0",
          "background": "#D79921",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b0",
          "background": "#FB4934",
          "type": "ruby",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": false
          },
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#1b1b1b",
          "powerline_symbol": "\ue0b0",
          "background": "#FABD2F",
          "type": "azfunc",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false
          },
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b0",
          "type": "aws",
          "style": "powerline",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FE8019{{end}}",
            "{{if contains \"jan\" .Profile}}#CC241D{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0ad ",
          "foreground": "#1b1b1b",
          "powerline_symbol": "\ue0b0",
          "background": "#FABD2F",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<transparent>\ue0b0</> \ueba2 {{ .FormattedMs }}\u2800",
          "foreground": "#FBF1C7",
          "background": "#665C54",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "<parentBackground>\ue0b0</> \ue23a ",
          "foreground": "#FBF1C7",
          "background": "#689D6A",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#FB4934{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#458588,transparent>\ue0b6</> \uf489 {{ .Name }} <transparent,#458588>\ue0b2</>",
          "foreground": "#FBF1C7",
          "background": "#458588",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "paused_icon": "\uf04c ",
            "playing_icon": "\uf04b "
          },
          "template": " \uf167 {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
          "foreground": "#1b1b1b",
          "powerline_symbol": "\ue0b2",
          "background": "#B8BB26",
          "type": "ytm",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
          "foreground": "#FBF1C7",
          "powerline_symbol": "\ue0b2",
          "background": "#D65D0E",
          "type": "battery",
          "style": "powerline",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#83A598{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FB4934{{end}}",
            "{{if eq \"Full\" .State.String}}#B8BB26{{end}}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#FBF1C7",
          "background": "#8EC07C",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}